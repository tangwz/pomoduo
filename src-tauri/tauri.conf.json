{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Pomoduo",
  "version": "0.1.0",
  "identifier": "com.terence.pomoduo",
  "build": {
    "beforeDevCommand": "npm run dev:web",
    "beforeBuildCommand": "npm run build:web",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Pomoduo",
        "label": "main",
        "width": 900,
        "height": 680,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
